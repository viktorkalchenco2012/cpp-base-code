#include<iostream>
#include<fstream>
#include<string>
using namespace std;
int main(){
  ofstream file("file.txt",ios_base::out);
  if(file.is_open())
  {
    file.cilose();
  }
  else
  {
    return 1;
	}
	file.open("file.txt");
	ifstream("file.txt",ios_base::in);
	if (file.is_open())
	{
		file<<"hello world"<<endl;
		file.close();
	}
	else
	{
	return 1;
	}
	return 0;
}
